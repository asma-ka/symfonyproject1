{% extends 'base.html.twig' %}

{% block title %}Client{% endblock %}
{% block content %}
<div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper">
            <div class="content-header row">
                <div class="content-header-left col-md-9 col-12 mb-2">
                    <div class="row breadcrumbs-top">
                        <div class="col-12">
                           
                        </div>
                    </div>
                </div>
                
            </div>


    <h1>Client</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ client.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ client.nom }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ client.prenom }}</td>
            </tr>
            <tr>
                <th>TypeIdentification</th>
                <td>{{ client.typeIdentification }}</td>
            </tr>
            <tr>
                <th>NumeroIdentification</th>
                <td>{{ client.numeroIdentification }}</td>
            </tr>
            <tr>
                <th>Adress</th>
                <td>{{ client.adress }}</td>
            </tr>
            <tr>
                <th>NumeroContrat</th>
                <td>{{ client.NumeroContrat }}</td>
            </tr>
           {#} <tr>
                <th>Dateprivisionnelle</th>
                <td>{{ client.dateprivisionnelle ? client.dateprivisionnelle|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Cordonees</th>
                <td>{{ client.cordonees }}</td>
            </tr>#}
        </tbody>
    </table>

    <a href="{{ path('client_index') }}">back to list</a>

    <a href="{{ path('client_edit', {'id': client.id}) }}">edit</a>

    {{ include('client/_delete_form.html.twig') }}
    </div>
{% endblock %}
